[Match]
Name=veth98

[Network]
IPv6PrivacyExtensions=yes
IPv6AcceptRA=no
DHCP=no
DHCPv6PrefixDelegation=yes
IPv6SendRA=yes

[DHCPv6PrefixDelegation]
UplinkInterface=veth99
SubnetId=9
Announce=yes
Token=eui64
Token=::1a:2b:3c:4d

[IPv6SendRA]
UplinkInterface=:none
EmitDNS=no
EmitDomains=no
